<%- contentFor('HeaderCss') %>


<%- contentFor('breadcrumb') %>
<ul class="list-inline menu-left mb-0">
    <li class="list-inline-item">
        <button type="button" class="button-menu-mobile open-left waves-effect">
            <i class="ion-navicon"></i>
        </button>
    </li>
    <li class="hide-phone list-inline-item app-search">
        <h3 class="page-title">Mission Control</h3>
    </li>
</ul>

<%- contentFor('body') %>

<div class="page-content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-10">
            <div class="row">
        <div class="col-md-6">
          <div class="mini-stat clearfix bg-white">
            <!-- <span class="mini-stat-icon bg-purple mr-0 float-right"><i class="mdi mdi-basket"></i></span> -->
            <div class="mini-stat-info">
              <span class="counter">Funnel (<%=pitchCount%> , $<%=pitchTotalCost%>)</span>
            </div>
            <div>
              <% projects.forEach(function(project) { %>
                <% if(project.phase_name == 'Pitch') { %>
                  
                  <span><button type="button" class="btn btn-light" onclick="loadProject('<%= project.id %>')" style="margin-bottom: 5px;"><%= project.project_name %></button></span>&nbsp;&nbsp;&nbsp;
                 
                <% } %>
              <% }); %>
             
              &nbsp;&nbsp;&nbsp;

            </div>

            <div class="clearfix"></div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="mini-stat clearfix bg-white">
            <div class="mini-stat-info">
              <span class="counter">Priority (<%=priorityCount%>, <%=priorityTotalCost%> PD)</span>
            </div>
            <div>
                <% projects.forEach(function(project) { %>
                    <% if(project.phase_name == 'Priority'){%>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" class="btn btn-light" style="margin-bottom: 5px;"><%= project.project_name %></button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <%}%>
                    
                <% }); %>

            </div>
            <div class="clearfix"></div>
          </div>
        </div>
        <div class="col-md-6">
            <div class="mini-stat clearfix bg-white">
              <!-- <span class="mini-stat-icon bg-purple mr-0 float-right"><i class="mdi mdi-basket"></i></span> -->
              <div class="mini-stat-info">
                <span class="counter">Discovery (<%=discoveryCount%> , $<%=discoveryTotalCost%>)</span>
              </div>
              <div>
                <% projects.forEach(function(project) { %>
                  <% if (project && project.phase_name === 'Discovery') { %>
                    <% if (project.health && typeof project.health === 'string') { %>
                      <% if (project.health.trim() === "Green") { %>
                        <span>
                          <button type="button" onclick="loadProject('<%= project.id %>')" 
                              style="width: 100%; border-color: green; border-width: 2px; margin-bottom: 5px;" 
                              class="btn btn-light text-left">
                              <%= project.project_name %>
                          </button>
                        </span>&nbsp;&nbsp;&nbsp;
                      <% } else if (project.health.trim() === "Yellow") { %>
                        <span>
                          <button type="button" onclick="loadProject('<%= project.id %>')" 
                              style="width: 100%; border-color: yellow; border-width: 2px; margin-bottom: 5px;" 
                              class="btn btn-light text-left">
                              <%= project.project_name %>
                          </button>
                        </span>&nbsp;&nbsp;&nbsp;
                      <% } else if (project.health.trim() === "Red") { %>
                        <span>
                          <button type="button" onclick="loadProject('<%= project.id %>')" 
                              style="width: 100%; border-color: red; border-width: 2px; margin-bottom: 5px;" 
                              class="btn btn-light text-left">
                              <%= project.project_name %>
                          </button>
                        </span>&nbsp;&nbsp;&nbsp;
                      <% } else { %>
                        <span>
                          <button type="button" onclick="loadProject('<%= project.id %>')" 
                              class="btn btn-light" style="margin-bottom: 5px;"><%= project.project_name %></button>
                        </span>&nbsp;&nbsp;&nbsp;
                      <% } %>
                    <% } %>
                  <% } %>
                <% }); %>
                
                
              </div>
              <div class="clearfix"></div>
            </div>
          </div>
          <div class="col-md-6">
            <div class="mini-stat clearfix bg-white">
              <div class="mini-stat-info">
                <span class="counter">Delivery (<%=deliveryCount%> , $<%=deliveryTotalCost%>)</span>
              </div>
              <div>
                <% projects.forEach(function(project) { %>
                  <% if (project.phase_name == 'Delivery' && project.phase_name !== null && project.phase_name !== undefined) { %>
                    
                    <% if (project.health && project.health.trim() === "Green") { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" 
                            style="width: 100%; border-color: green; border-width: 2px; margin-bottom: 5px;" 
                            class="btn btn-light text-left">
                            <%= project.project_name %>
                        </button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } else if (project.health && project.health.trim() === "Yellow") { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" 
                            style="width: 100%; border-color: yellow; border-width: 2px; margin-bottom: 5px;" 
                            class="btn btn-light text-left">
                            <%= project.project_name %>
                        </button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } else if (project.health && project.health.trim() === "Red") { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" 
                            style="width: 100%; border-color: red; border-width: 2px; margin-bottom: 5px;" 
                            class="btn btn-light text-left">
                            <%= project.project_name %>
                        </button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } else { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" class="btn btn-light" style="margin-bottom: 5px;"><%= project.project_name %></button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } %>
                    
                  <% } else { %>
                    <span>
                      <button type="button" onclick="loadProject('<%= project.id %>')" class="btn btn-light" style="margin-bottom: 5px;"><%= project.project_name %></button>
                    </span>&nbsp;&nbsp;&nbsp;
                  <% } %>
                <% }); %>
                
              </div>
              <div class="clearfix"></div>
            </div>
          </div>
          <div class="col-12">
            <div class="mini-stat clearfix bg-white">
              <div class="mini-stat-info">
                <span class="counter">Operations (<%=operationsCount%> , $<%=operationsTotalCost%>)</span>
              </div>
              <div>
                <% projects.forEach(function(project) { %>
                  <% if (project.phase_name === 'Operations') { %>
                   
                    <% if (project.health.trim() === "Green") { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" 
                            style="width: 100%; border-color: green; border-width: 2px; margin-bottom: 5px;" 
                            class="btn btn-light text-left">
                            <%= project.project_name %>
                        </button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } else if (project.health.trim() === "Yellow") { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" 
                            style="width: 100%; border-color: yellow; border-width: 2px; margin-bottom: 5px;" 
                            class="btn btn-light text-left">
                            <%= project.project_name %>
                        </button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } else if (project.health.trim() === "Red") { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" 
                            style="width: 100%; border-color: red; border-width: 2px; margin-bottom: 5px;" 
                            class="btn btn-light text-left">
                            <%= project.project_name %>
                        </button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } else { %>
                      <span>
                        <button type="button" onclick="loadProject('<%= project.id %>')" class="btn btn-light" style="margin-bottom: 5px;"><%= project.project_name %></button>
                      </span>&nbsp;&nbsp;&nbsp;
                    <% } %>
                  <% } %>
              <% }); %>
                      </div>
              <div class="clearfix"></div>
            </div>
          </div>
        </div>
        </div>
        <!-- radar -->
        <div class="col-md-2">
  <div class="mini-stat clearfix bg-white text-center">
    <div>
      <div>
        <button
          type="button"
          style="width: 75%; margin-bottom: 5px"
          class="btn btn-light"
          onClick=loadRadar();>
          Radar
        </button>
      </div>
      <div>
        <button
          type="button"
          style="width: 75%; margin-bottom: 5px"
          class="btn btn-light"
          onClick=loadFlightPlan()>
          Flight Plan
        </button>
      </div>
      <div>
        <button
          type="button"
          style="width: 75%; margin-bottom: 5px"
          class="btn btn-light"
          onClick=loadHealth()>
          Health
        </button>
      </div>
      <div class="col-6">
      
      </div>
    </div>
    <div >
      <div class="row text-center" style="padding-top:20px;">
        <div class="col-md-12 text-center">
          <strong>$$</strong>
          <ul class="list-unstyled">
              <li>Full: <%=totalCostSum%></li>
              <li>Used: <%=totalCostUsed%></li>
              <li>Avail: <%=totalAvailableCost%></li>
          </ul>
        
          <strong>Effort</strong>
        <ul class="list-unstyled">
            <li>40K PH</li>
            <li>35K PH</li>
            <li>5K PH</li>
        </ul>
        </div>
    </div>
  </div>
   
</div>

   

<%- contentFor('FooterJs') %>
<script src="public/assets/plugins/peity-chart/jquery.peity.min.js"></script>
<!--C3 Chart-->
<script src="public/assets/plugins/d3/d3.min.js"></script>
<script src="public/assets/plugins/c3/c3.min.js"></script>
<!-- KNOB JS -->
<script src="public/assets/plugins/jquery-knob/excanvas.js"></script>
<script src="public/assets/plugins/jquery-knob/jquery.knob.js"></script>
<!-- Page specific js -->
<script src="public/assets/pages/dashboard.js"></script>

 <%- contentFor('BottomJs') %>

 <script>
  function loadProject(project_id){
   
    window.location.replace('/projects/cockpit/'+project_id);
   
  }
  function loadHealth(){
   window.location.replace('/projects/health/view/');
  }
  function loadRadar(){
   window.location.replace('/projects/radar/view/');
   
  }
  function loadFlightPlan(){
   window.location.replace('/projects/flight/view/');
  }

 </script>
 
