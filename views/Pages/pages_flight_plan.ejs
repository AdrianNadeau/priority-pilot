    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                    <img src="/public/assets/images/gannt.png" alt="Flight Plan" class="img-fluid" style="width:80%;height:90%;">
                </div>
            </div>
        </div>

